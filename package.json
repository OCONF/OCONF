{
  "name": "OCONF",
  "version": "0.0.0",
  "description": "Video conferencing software for the classroom",
  "main": "index.js",
  "repository": "git@github.com:OCONF/OCONF.git",
  "scripts": {
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start": "webpack --progress --colors --watch -d",
    "build": "webpack --progress --colors -p",
    "server": "node bin/www",
    "local": "npm run dev & npm run server",
    "test": "jest",
    "lint": "eslint ./src"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testDirectoryName": "spec"
  },
  "author": "OCONF",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-jest": "*",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.16.1",
    "jest-cli": "*",
    "react-addons-test-utils": "^0.14.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "babel-register": "^6.4.3",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "hark": "^1.1.3",
    "lodash": "^3.10.1",
    "morgan": "~1.6.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "rtcmulticonnection-v3": "^3.2.87",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.4"
  }
}
